<ruleset name="Fedora Project">
  <target host="fedoraproject.org" />
  <target host="*.fedoraproject.org" />

  <rule from="^http://(www\.)?fedoraproject\.org/" to="https://fedoraproject.org/" />
  <rule from="^http://(alt|archives|blogs|boot|docs|lists|mirrors|spins|start|talk)\.fedoraproject\.org/" to="https://$1.fedoraproject.org/" />
</ruleset>